# Add your Discord bot token here
DISCORD_TOKEN=your_token_here

# AI Provider Configuration
# Choose provider: 'google' or 'ollama'
AI_PROVIDER=google

# Google AI Configuration (required if AI_PROVIDER=google)
GOOGLE_API_KEY=your_google_api_key_here

# Ollama Configuration (required if AI_PROVIDER=ollama)
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=llama2

# Bot Configuration
# Set to 'false' to make bot responses visible to everyone (default: true)
EPHEMERAL_MESSAGES=true